<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:library="my.control.library."
	height="100%"
	controllerName="my.control.library.sample.ListNoDataImage.Page">
    <Page showHeader="false" class="sapUiContentPadding">
		<library:List id="list" items="{sample>/}" showSeparators="Tile"
					headerText="Users"
					noDataText="No data available"
					  noDataImage="{local>/imagePath}">
			<library:noDataContent>
				<Label text="No data control"/>
			</library:noDataContent>
			<CustomListItem>
				<Label class="label" text="{sample>text}"/>
			</CustomListItem>
		</library:List>
    </Page>
</mvc:View>
